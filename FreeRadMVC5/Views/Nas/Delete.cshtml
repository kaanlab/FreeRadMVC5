@model FreeRadMVC5.Models.Nas

@{
    ViewBag.Title = "Удалить устройство";
}

<h2>Удалить</h2>

<div>
    <h4>Nas</h4>
    <hr />
    <div class="alert alert-danger" role="alert">
        Вы уверенны, что хотите удалить сетевое устройство "<b>@Html.DisplayFor(model => model.NasName)</b>"?
    </div>
    <div class="well">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.NasName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.NasName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.ShortName)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ShortName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Type)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Type)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Ports)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Ports)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Secret)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Secret)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Community)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Community)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Description)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Description)
            </dd>

        </dl>
    </div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-danger" /> |
            @Html.ActionLink("Отменить", "Index", null, new { @class = "btn btn-default" })
        </div>
    }
</div>
